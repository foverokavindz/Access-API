{
	"ConnectionStrings": {
		"DefaultConnection": "Host=localhost;Database=marketplace_results;Username=your_username;Password=your_password"
	},
	"Cors": {
		"AllowedOrigins": ["http://localhost:3000", "http://localhost:8080", "http://localhost:5173", "https://localhost:3000"],
		"AllowCredentials": true,
		"AllowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
		"AllowedHeaders": ["*"]
	},
	"RateLimiting": {
		"EnableRateLimiting": true,
		"PermitLimit": 100,
		"WindowInSeconds": 60,
		"QueueLimit": 10
	},
	"Caching": {
		"DefaultExpirationMinutes": 15,
		"HealthCheckCacheMinutes": 1
	},
	"HealthChecks": {
		"EnableUI": true,
		"UIPath": "/health-ui",
		"CheckInterval": 30
	},
	"ApiSettings": {
		"DefaultPageSize": 50,
		"MaxPageSize": 100,
		"ApiTitle": "Results Service API",
		"ApiVersion": "v1",
		"ApiDescription": "API for managing marketplace scraping results"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning",
			"Microsoft.EntityFrameworkCore.Database.Command": "Information",
			"resultsService.Presentation.Middleware.RequestLoggingMiddleware": "Information",
			"resultsService.Presentation.Middleware.GlobalExceptionHandlingMiddleware": "Error"
		},
		"Console": {
			"IncludeScopes": true,
			"TimestampFormat": "yyyy-MM-dd HH:mm:ss "
		}
	},
	"AllowedHosts": "*"
}
